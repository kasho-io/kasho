{
  "name": "@kasho/console",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "test": "playwright test",
    "test:unit": "jest --watchAll=false",
    "test:unit:watch": "jest --watch",
    "test:unit:coverage": "jest --coverage",
    "test:headed": "playwright test --headed",
    "test:ui": "playwright test --ui",
    "test:ci": "npm run prettier:check && npm run lint && npm run type-check && npm run test:unit",
    "test:e2e": "npm run test",
    "type-check": "npx tsc --noEmit",
    "prettier": "npx prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "prettier:check": "npx prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "prettier:write": "npx prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\""
  },
  "dependencies": {
    "@vercel/blob": "^1.1.1",
    "@workos-inc/authkit-nextjs": "^2.7.0",
    "@workos-inc/node": "^7.69.2",
    "daisyui": "^5.0.37",
    "next": "^15.3.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "eslint": "^9.35.0",
    "eslint-config-next": "^15.5.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-node": "^10.9.2"
  }
}
