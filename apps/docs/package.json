{
  "name": "docs",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--require ./scripts/polyfill-localstorage.js' next dev",
    "build": "NODE_OPTIONS='--require ./scripts/polyfill-localstorage.js' next build",
    "start": "next start",
    "lint": "eslint .",
    "test": "echo \"No tests yet\" && exit 0",
    "test:ci": "npm run prettier:check && npm run lint && npm run type-check && npm run test",
    "type-check": "npx tsc --noEmit",
    "prettier": "npx prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "prettier:check": "npx prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "postbuild": "npx pagefind --site .next/server/app --output-path public/_pagefind"
  },
  "dependencies": {
    "@napi-rs/simple-git": "^0.1.19",
    "next": "^15.3.4",
    "nextra": "^4.2.17",
    "nextra-theme-docs": "^4.2.17",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "eslint": "^9.35.0",
    "eslint-config-next": "^15.5.3",
    "pagefind": "^1.3.0"
  },
  "overrides": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  }
}
