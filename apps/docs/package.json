{
  "name": "docs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "echo \"No tests yet\" && exit 0",
    "test:ci": "npm run prettier:check && npm run lint && npm run type-check && npm run test",
    "type-check": "npx tsc --noEmit",
    "prettier": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "prettier:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "postbuild": "pagefind --site .next/server/app --output-path public/_pagefind"
  },
  "dependencies": {
    "next": "^15.3.4",
    "nextra": "^4.2.17",
    "nextra-theme-docs": "^4.2.17",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "24.0.10",
    "@types/react-dom": "^19",
    "@types/react": "19.1.8",
    "eslint-config-next": "15.3.5",
    "eslint-config-prettier": "^10.1.5",
    "eslint": "9.30.1",
    "pagefind": "^1.3.0",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.17",
    "typescript": "^5"
  }
}
